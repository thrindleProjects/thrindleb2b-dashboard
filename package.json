{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install",
    "watch:pug": "npx tailwindcss -i ./src/styles/globals.css -o ./src/templates/pug.css --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.4",
    "@reduxjs/toolkit": "^1.9.5",
    "@sparticuz/chromium": "112.0.2",
    "@szhsin/react-menu": "^4.0.0",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.4.0",
    "chart.js": "^4.3.0",
    "chrome-aws-lambda": "^10.1.0",
    "clsx": "^1.2.1",
    "flowbite-react": "^0.4.7",
    "formik": "^2.4.0",
    "framer-motion": "^10.12.16",
    "jspdf": "^2.5.1",
    "next": "13.4.3",
    "next-auth": "^4.22.1",
    "node-sass": "^7.0.3",
    "pug": "^3.0.2",
    "puppeteer": "^20.8.1",
    "puppeteer-core": "19.8.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.8.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-select": "^5.7.3",
    "redux-persist": "^6.0.0",
    "sass": "^1.62.1",
    "styled-components": "^6.0.0-rc.2",
    "tailwind-merge": "^1.12.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@faker-js/faker": "^8.0.1",
    "@iconify/react": "^4.1.0",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/pug": "^2.0.6",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^18.2.6",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.7.5",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
